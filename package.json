{
  "name": "ant-capsulizer",
  "version": "1.0.0",
  "description": "capsulization agent for ant",
  "main": "index.js",
  "scripts": {
  "dev": "nodemon src/index.js",
  "report": "node src/tools/logReport.js",
  "requeue": "node src/tools/requeueFailedJobs.js",
  "schedule": "node src/tools/scheduleUpdates.js",
  "seed": "node src/seed.js",
  "worker": "node src/worker.js",
  "dev:seed": "concurrently -n \"WORKER,SEED\" -c \"cyan,yellow\" \"node src/worker.js\" \"bash -c 'sleep 3 && node src/seed.js'\""
},
  "keywords": [],
  "author": "Steve Rouse",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "bullmq": "^5.61.0",
    "cheerio": "^1.1.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "fast-sha256": "^1.3.0",
    "fs-extra": "^11.3.2",
    "ioredis": "^5.8.1",
    "mysql2": "^3.15.2",
    "playwright": "^1.56.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10"
  }
}
